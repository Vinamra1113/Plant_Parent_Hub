<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_Acquired_Cannot_Be_Future</fullName>
    <active>true</active>
    <errorConditionFormula>Date_Acquired__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Date_Acquired__c</errorDisplayField>
    <errorMessage>The &apos;Date Acquired&apos; cannot be in the future.</errorMessage>
</ValidationRule>
