<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Last_Watered_Date_Cannot_Be_Future</fullName>
    <active>true</active>
    <errorConditionFormula>Last_Watered_Date__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Last_Watered_Date__c</errorDisplayField>
    <errorMessage>The &apos;Last Watered Date&apos; cannot be in the future.</errorMessage>
</ValidationRule>
