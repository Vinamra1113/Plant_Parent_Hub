<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Log_Date_Cannot_Be_In_Future</fullName>
    <active>true</active>
    <errorConditionFormula>Log_Date__c &gt; NOW()</errorConditionFormula>
    <errorDisplayField>Log_Date__c</errorDisplayField>
    <errorMessage>The log date and time cannot be in the future.</errorMessage>
</ValidationRule>
